{
    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Attach to ALL Python Functions",
            "type": "python",
            "request": "attach",
            "port": 9091,
            "preLaunchTask": "func: host start"
        },
        {
            "name": "Attach to Python Function: booking_scheduler",
            "type": "python",
            "request": "attach",
            "port": 9091,
            "preLaunchTask": "func: host start --functions booking_scheduler"
        },
        {
            "name": "Attach to Python Function: golfnow_teetimebooker",
            "type": "python",
            "request": "attach",
            "port": 9091,
            "preLaunchTask": "func: host start --functions golfnow_teetimebooker"
        },
        {
            "name": "Attach to Python Function: smith_teetimebooker",
            "type": "python",
