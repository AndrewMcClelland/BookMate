{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Attach to ALL Python Functions",
            "type": "python",
            "request": "attach",
            "port": 9091,
            "preLaunchTask": "func: host start"
        },
        {
            "name": "Attach to Python Function: BookingScheduler",
            "type": "python",
            "request": "attach",
            "port": 9091,
            "preLaunchTask": "func: host start --functions BookingScheduler"
        },
        {
            "name": "Attach to Python Function: GolfNowTeeTimeBooker",
            "type": "python",
